{
  "manifest_version": 3,
  "name": "YT Music Swap Like/Dislike",
  "version": "1.0",
  "description": "Swaps the Like and Dislike buttons on YouTube Music",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://music.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
